# n8n Environment Variables for Different Not Less Apparel Workflow
# Copy this file to .env and fill in your actual values

# Webhook Authentication
WEBHOOK_API_KEY=your-secure-webhook-key

# fal.ai API
FALAI_API_KEY=your-falai-api-key

# Printify API
PRINTIFY_API_KEY=your-printify-api-key
PRINTIFY_SHOP_ID=your-printify-shop-id

# Airtable API
AIRTABLE_API_KEY=your-airtable-api-key
AIRTABLE_BASE_ID=your-airtable-base-id

# n8n Configuration
N8N_ENCRYPTION_KEY=your-secure-encryption-key
N8N_HOST=n8n
N8N_PORT=5678
N8N_PROTOCOL=http

# Optional: Uncomment and set these for production environments
# N8N_PROTOCOL=https
# N8N_SSL_KEY=/path/to/ssl/key.pem
# N8N_SSL_CERT=/path/to/ssl/cert.pem

# Optional: Uncomment for basic authentication
# N8N_BASIC_AUTH_ACTIVE=true
# N8N_BASIC_AUTH_USER=admin
# N8N_BASIC_AUTH_PASSWORD=secure-password
